generator client {
  provider = "prisma-client"
  output   = "../../generated/prisma"
}

generator prismabox {
  provider                     = "prismabox"
  typeboxImportDependencyName  = "elysia"
  typeboxImportVariableName    = "t"
  inputModel                   = true
  output                       = "../../generated/prismabox"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
